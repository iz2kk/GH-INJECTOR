		$PL_ProcessList_Y = MakeShort(BitAND(BitShift($lParam, 0x10), 0xFFFF), $g_ShortStruct)
	EndIf

	Return $GUI_RUNDEFMSG

EndFunc   ;==>WM_MOVE

